{
    "editor": {
        "fontFamily": "'Operator-Caska', 'Cascadia Code', 'CaskaydiaCove Nerd Font', 'Fira Code', 'Droid Sans Mono', 'monospace'",
        "fontSize": 14.5,
        "lineHeight": 2,
        "ursorBlinking": "expand",
        "cursorWidth": 2,
        "linkedEditing": true,
        "roundedSelection": true,
        "formatOnSave": true,
        "mouseWheelScrollSensitivity": 2,
        "accessibilitySupport": "off",
        "fontLigatures": true,
        "formatOnType": true,
        "formatOnPaste": true,
        "renderLineHighlight": "none",
        "smoothScrolling": true,
        "foldingImportsByDefault": true,
        "cursorSmoothCaretAnimation": "on",
        "overviewRulerBorder": false,
        "hideCursorInOverviewRuler": true,
        "bracketPairColorization.enabled": true,
        "defaultFormatter": "esbenp.prettier-vscode",
        "hover": {
            "enabled": true,
            "delay": 1000,
            "sticky": true
        },
        "stickyScroll": {
            "enabled": false
        },
        "minimap": {
            "enabled": true,
            "size": "fill",
            "renderCharacters": true,
            "showSlider": "always",
            "maxColumn": 50,
            "side": "right"
        },
        "find": {
            "autoFindInSelection": "always",
            "addExtraSpaceOnTop": false,
            "seedSearchStringFromSelection": true,
            "cursorMoveOnType": true
        },
        "scrollbar": {
            "vertical": "auto",
            "horizontal": "auto",
            "useShadows": true,
            "verticalHasArrows": false,
            "horizontalHasArrows": false,
            "verticalScrollbarSize": 8,
            "horizontalScrollbarSize": 8,
            "arrowSize": 30,
            "scrollByPage": true
        },
        "parameterHints": {
            "enabled": true,
            "cycle": true
        }
    },
    "files": {
        "autoSave": "afterDelay",
        "watcherExclude": {
            "**/node_modules/*": true,
            "**/build/*": true,
            "**/dist/*": true,
            "**/.git/*": true,
            "**/logs/*": true,
            "**/.vscode/*": true
        },
        "exclude": {
            "**/.classpath": true,
            "**/.project": true,
            "**/.settings": true,
            "**/.factorypath": true
        }
    },
    "background": {
        "enabled": true,
        "useFront": false,
        "style": {
            "content": "''",
            "pointer-events": "none",
            "position": "absolute",
            "z-index": "99999",
            "width": "40%",
            "height": "40%",
            "background-position": "center",
            "background-size": "cover",
            "background-repeat": "no-repeat",
            "opacity": 0.1,
            "left": "30%",
            "top": "10rem"
        },
        "customImages": ["https://i.postimg.cc/LXQ42V13/vscodelow.png"]
    },
    "search": {
        "exclude": {
            "**/node_modules": true,
            "**/dist": true,
            "**/build": true,
            "**/.git": true
        },
        "seedWithNearestWord": true
    },
    "powermode": {
        "enabled": false,
        "presets": "flames",
        "combo": {
            "keybinding": "ctrl",
            "enabled": true,
            "counterSize": 0
        },
        "explosions": {
            "enabled": true,
            "explosionHeight": 0,
            "explosionWidth": 0
        },
        "shake": {
            "enabled": false
        }
    },
    "better-comments": {
        "highlightPlainText": true,
        "tags": [
            { "tag": "*", "color": "#000000", "bold": true, "backgroundColor": "#198754" },
            { "tag": "?", "color": "#000000", "bold": true, "backgroundColor": "#FFC107" },
            { "tag": "~", "color": "#FFFFFF", "bold": true, "backgroundColor": "#6610F2" },
            { "tag": "!", "color": "#000000", "bold": true, "backgroundColor": "#DC3545" },
            { "tag": "/", "color": "#000000", "bold": true, "backgroundColor": "#00b7ffec" },
            { "tag": "-1", "color": "#ffffff", "bold": true, "backgroundColor": "#D61355" }
        ]
    },
    "prettier": {
        "withNodeModules": true,
        "enableDebugLogs": true,
        "arrowParens": "always",
        "bracketSameLine": true,
        "bracketSpacing": true,
        "embeddedLanguageFormatting": "auto",
        "htmlWhitespaceSensitivity": "ignore",
        "insertPragma": false,
        "jsxSingleQuote": false,
        "proseWrap": "preserve",
        "quoteProps": "as-needed",
        "requirePragma": false,
        "semi": true,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "all",
        "useTabs": false,
        "vueIndentScriptAndStyle": false,
        "printWidth": 125
    },
    "terminal": {
        "integrated": {
            "fontFamily": "'CaskaydiaCove Nerd Font', 'Cascadia Code', 'Fira Code', 'Droid Sans Mono', 'monospace'",
            "fontSize": 13,
            "fontWeight": "normal",
            "fontLigatures": true,
            "cursorBlinking": true,
            "scrollback": 10000,
            "cursorWidth": 2,
            "cursorStyle": "underline",
            "lineHeight": 1.2,
            "letterSpacing": 1,
            "allowMnemonics": true,
            "copyOnSelection": false,
            "fastScrollSensitivity": 4,
            "enableMultiLinePasteWarning": "auto",
            "enableVisualBell": true,
            "clear": "ctrl+l",
            "showExitAlert": true,
            "showOnRightClick": true,
            "suggestOnTriggerCharacters": true,
            "wordBasedSuggestions": true,
            "defaultProfile": {
                "windows": "Git Bash",
                "osx": "zsh",
                "linux": "zsh"
            },
            "shellArgs": {
                "linux": ["-l"],
                "osx": ["-l"],
                "windows": ["-l"]
            },
            "shell": {
                "windows": "C:\\Program Files\\Git\\bin\\bash.exe",
                "osx": "/bin/zsh",
                "linux": "/bin/zsh"
            }
        },
        "sourceControlRepositoriesKind": "both",
        "explorerKind": "both",
        "external": {
            "linuxExec": "x-terminal-emulator",
            "osxExec": "Terminal.app",
            "windowsExec": "C:\\Windows\\System32\\cmd.exe"
        }
    },
    "cSpell": {
        "userWords": ["arif", "azmarif", "আরিফ", "cascadia", "null"],
        "language": "en,en-GB,en-US,en-BN"
    },
    "liveServer": {
        "settings": {
            "donotVerifyTags": true,
            "ChromeDebuggingAttachment": true
        }
    },
    "autoHide": {
        "autoHideSideBar": false,
        "autoHidePanel": false,
        "sideBarDelay": 0
    },
    "notebook": {
        "outline": {
            "showCodeCells": true
        },
        "consolidatedRunButton": true
    },
    "explorer": {
        "confirmDelete": false,
        "confirmDragAndDrop": true
    },
    "debug": {
        "console": {
            "acceptSuggestionOnEnter": "on",
            "fontSize": 13,
            "fontFamily": "'CaskaydiaCove Nerd Font', 'Cascadia Code', 'Fira Code', 'Droid Sans Mono', 'monospace'",
            "showBreakpointsInOverviewRuler": true,
            "showSubSessionsInToolBar": true
        }
    },
    "workbench": {
        "iconTheme": "vscode-icons",
        "editorAssociations": {
            "*.ico": "default"
        },
        "editor": {
            "sharedViewState": true,
            "fontSize": 14.5,
            "fontLigatures": true,
            "lineNumbers": "on",
            "lineNumbersMinChars": 3,
            "renderControlCharacters": false,
            "renderIndentGuides": true,
            "renderLineHighlight": "all",
            "renderWhitespace": "none",
            "scrollBeyondLastLine": true,
            "smoothScrolling": true,
            "wordWrap": "on",
            "wordWrapColumn": 80,
            "wordWrapMinified": true,
            "scrollbar": {
                "vertical": "auto",
                "horizontal": "auto"
            },
            "empty": {
                "hint": "hidden"
            }
        },
        "layoutControl": {
            "enabled": false
        },
        "colorTheme": "Anti-Glare - Official"
    },
    "window": {
        "customTitleBarVisibility": "never",
        "menuBarVisibility": "toggle",
        "commandCenter": true,
        "title": "A. Z. M. Arif",
        "confirmBeforeClose": "always"
    },
    "typescript": {
        "suggest": {
            "completeFunctionCalls": true
        },
        "inlayHints": {
            "parameterTypes": {
                "enabled": false
            },
            "typeHints": {
                "enabled": "all"
            },
            "parameterNames": {
                "enabled": "all"
            }
        },
        "format": {
            "placeOpenBraceOnNewLineForFunctions": true
        }
    },
    "javascript": {
        "inlayHints": {
            "parameterNames": {
                "enabled": "none"
            }
        },
        "autoClosingTags": true,
        "suggest": {
            "enabled": true
        },
        "updateImportsOnFileMove": {
            "enabled": "always"
        },
        "format": {
            "enable": true,
            "placeOpenBraceOnNewLineForFunctions": true
        }
    },
    "eslint": {
        "format": {
            "enable": true
        },
        "debug": true,
        "lintTask": {
            "enable": true
        },
        "nodeEnv": ""
    },
    "json": {
        "schemaDownload": {
            "enable": true
        },
        "schemas": []
    },
    "code-runner": {
        "runInTerminal": true,
        "clearPreviousOutput": false
    },
    "markdown": {
        "preview": {
            "typographer": true
        }
    },
    "vsicons": {
        "presets": {
            "jsonOfficial": true
        },
        "dontShowNewVersionMessage": true
    },
    "output": {
        "smartScroll": {
            "enabled": true,
            "skipFocusedEditor": true
        }
    },
    "tabnine": {
        "experimentalAutoImports": true
    },
    "emmet": {
        "showAbbreviationSuggestions": true,
        "showSuggestionsAsSnippets": true,
        "triggerExpansionOnTab": true,
        "includeLanguages": {
            "javascript": "javascriptreact"
        }
    },
    "html-css-class-completion": {
        "enableEmmetSupport": true
    },
    "projectManager": {
        "filterOnFullPath": true,
        "openInNewWindowWhenClickingInStatusBar": true,
        "showParentFolderInfoOnDuplicates": true
    },
    "git": {
        "autofetch": true,
        "openRepositoryInParentFolders": "always"
    },
    "accessibility": {
        "screenReaderMode": "off",
        "autoDetectHighContrast": true,
        "highContrastTheme": "hc-black",
        "sound": {
            "terminalBell": "off"
        }
    },
    "diffEditor": {
        "wordWrap": "on",
        "ignoreTrimWhitespace": true
    }
}
